pipeline {
  
  agent any
  
  stages {
    
    stage("build") {
      
      steps{
        echo 'it was build'
      }
    }
    
    stage("test") {
      
      steps{
        echo 'it was test'
      }
    }
    
    stage("deploy") {
      
      steps{
        echo 'it was deploy'
      }
    }
  }
}
